<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/menuslidingpanel"
    android:layout_width="350dp"
    android:layout_height="500dp"
    android:layout_centerInParent="true"
    android:background="@drawable/dictionary_background"
    android:orientation="vertical"
    android:visibility="visible"

    >


    <Button
        android:id="@+id/btn_m_s_1"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@drawable/button_monster_a1"
        android:onClick="card_1"
        android:layout_above="@+id/btn_m_s_6"
        android:layout_toLeftOf="@+id/button17"
        android:layout_toStartOf="@+id/button17"
        android:layout_marginBottom="14dp" />

    <Button
        android:id="@+id/btn_m_s_2"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@drawable/button_monster_a2"
        android:onClick="card_2"
        android:layout_alignBaseline="@+id/btn_m_s_1"
        android:layout_alignBottom="@+id/btn_m_s_1"
        android:layout_alignLeft="@+id/btn_m_s_5"
        android:layout_alignStart="@+id/btn_m_s_5" />

    <Button
        android:id="@+id/btn_m_s_3"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@drawable/button_monster_a3"
        android:onClick="card_3"
        android:layout_alignBaseline="@+id/btn_m_s_2"
        android:layout_alignBottom="@+id/btn_m_s_2"
        android:layout_toRightOf="@+id/button17"
        android:layout_toEndOf="@+id/button17" />

    <Button
        android:id="@+id/btn_m_s_4"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@drawable/button_monster_a4"
        android:onClick="card_4"
        android:layout_alignBaseline="@+id/btn_m_s_6"
        android:layout_alignBottom="@+id/btn_m_s_6"
        android:layout_alignLeft="@+id/btn_m_s_1"
        android:layout_alignStart="@+id/btn_m_s_1" />

    <Button
        android:id="@+id/btn_m_s_5"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@drawable/button_monster_a5"
        android:onClick="card_5"
        android:layout_alignBaseline="@+id/btn_m_s_4"
        android:layout_alignBottom="@+id/btn_m_s_4"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/btn_m_s_6"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@drawable/button_monster_a6"
        android:onClick="card_6"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/btn_m_s_3"
        android:layout_alignStart="@+id/btn_m_s_3" />

    <Button
        android:id="@+id/btn_m_s_7"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@drawable/button_monster_a7"
        android:onClick="card_7"
        android:layout_alignBaseline="@+id/btn_m_s_9"
        android:layout_alignBottom="@+id/btn_m_s_9"
        android:layout_alignLeft="@+id/btn_m_s_4"
        android:layout_alignStart="@+id/btn_m_s_4" />

    <Button
        android:id="@+id/btn_m_s_8"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@drawable/button_monster_a8"
        android:onClick="card_8"
        android:layout_alignBaseline="@+id/btn_m_s_7"
        android:layout_alignBottom="@+id/btn_m_s_7"
        android:layout_alignLeft="@+id/btn_m_s_5"
        android:layout_alignStart="@+id/btn_m_s_5" />

    <Button
        android:id="@+id/btn_m_s_9"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@drawable/button_monster_a9"
        android:onClick="card_9"
        android:layout_below="@+id/btn_m_s_5"
        android:layout_alignLeft="@+id/btn_m_s_6"
        android:layout_alignStart="@+id/btn_m_s_6"
        android:layout_marginTop="17dp" />

    <Button
        android:id="@+id/button15"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/button_left"
        android:onClick="left"
        android:layout_alignTop="@+id/btn_m_s_5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/button16"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/button_right"
        android:onClick="right"
        android:layout_alignTop="@+id/btn_m_s_5"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:id="@+id/button17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/button_confirm"
        android:onClick="confirm"
        android:layout_marginTop="37dp"
        android:layout_below="@+id/btn_m_s_8"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/image_null"
        android:layout_centerVertical="true"
        android:layout_alignRight="@+id/btn_m_s_3"
        android:layout_alignEnd="@+id/btn_m_s_3" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="confirm_explain"
        android:layout_alignTop="@+id/button17"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="12dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginBottom="30dp"
        app:srcCompat="@drawable/text_diction_m"
        android:layout_above="@+id/btn_m_s_1"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView5"
        android:layout_toEndOf="@+id/imageView5"
        android:layout_toRightOf="@+id/imageView5"
        android:onClick="btn_mc_change" />

</RelativeLayout>